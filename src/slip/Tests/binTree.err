emptyTree(){ return(new/0) ; }

inTree/0(x){ return (0) ; }

inTree/3(x)
{ 
  if (x == this.3) return (1) ;
  if (x < this.3)  y = this.1 ; else y = this.2 ; 
  return (y.inTree(x)) ; 
}
  
addToTree/0(x)
{ 
  y = new/3 ;
  y.1 = this ;
  y.2 = this ;
  y.3 = x ;
  return(y);
}

addToTree/3(x)
{ 
  if (x < this.3)
    {y = this.1 ; this.1 = y.addToTree(x) ; }
  else if (this.3 < x)
    {y = this.2 ; this.2 = y.addToTree(x) ; }
  return (this) ; 
}

writeTree/0(){ }

writeTree/3()
{
  x = this.1 ;
  x = x.writeTree() ;
  write(this.3) ;
  x = this.2 ;
  x = x.writeTree() ;
}

